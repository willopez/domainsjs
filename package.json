{
  "name": "domainsjs",
  "version": "0.0.1",
  "description": "A domain name manager",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/willopez/domainsjs.git"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "node",
    "mysql"
  ],
  "author": "Will Lopez",
  "license": "MIT",
  "homepage": "https://github.com/willopez/domainsjs",
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "start": "NODE_ENV=production ./node_modules/.bin/babel-node app",
    "dev": "./node_modules/.bin/nodemon -e js,jsx,json -w app.js -w server -i client -x node_modules/.bin/babel-node app",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "compile": "rimraf dist/* && cp webpack.config.js dist/ && ./node_modules/.bin/babel app.js -o dist/app.js && ./node_modules/.bin/babel index.js -o dist/index.js && ./node_modules/.bin/babel server -d dist/server && ./node_modules/.bin/babel client -d dist/client && ./node_modules/.bin/babel shared -d dist/shared"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "classnames": "^2.2.5",
    "express": "^4.13.3",
    "express-graphql": "^0.4.5",
    "faker": "^3.0.1",
    "fixed-data-table": "^0.6.0",
    "graphql": "^0.4.14",
    "history": "^1.17.0",
    "invariant": "^2.2.0",
    "lodash": "^4.0.0",
    "moment": "^2.11.1",
    "morgan": "^1.6.1",
    "mysql": "^2.13.0",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "react": "0.14.9",
    "react-dom": "0.14.9",
    "react-highcharts": "^6.0.0",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-toggle": "2.1.0",
    "redux": "^3.0.5",
    "redux-combine-actions": "^0.1.2",
    "redux-logger": "^2.3.2",
    "redux-router": "1.0.0-beta5",
    "redux-thunk": "^1.0.3",
    "sequelize": "^3.17.1",
    "serialize-javascript": "^1.1.2",
    "superagent": "^1.6.1",
    "superagent-bluebird-promise": "^3.0.0",
    "warning": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^5.8.34",
    "babel-polyfill": "^6.0.16",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "babelify": "^6.3.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.4.2",
    "expect": "^1.13.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "graphiql": "^0.4.5",
    "html-webpack-plugin": "^1.6.1",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.6",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "rimraf": "^2.4.3",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
